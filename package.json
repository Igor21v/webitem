{
    "name": "production-project",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "webpack serve --env port=3000",
        "start:vite": "vite",
        "start:dev:server": "node ./json-server/index.js",
        "start:dev": "npm-run-all --parallel  start:dev:server start",
        "start:dev:vite": "npm-run-all --parallel  start:dev:server start:vite",
        "build:prod": "webpack --env mode=production apiUrl=https://production-project-server-two.vercel.app/",
        "build:prod:ba": "npm run build:prod bundleAnalyzer=true",
        "build:dev": "webpack --env mode=development",
        "prettier": "npx prettier --write **/*.{ts,tsx,json}",
        "lint:ts": "npx eslint \"**/*.{ts,tsx}\"",
        "lint:ts:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
        "lint:scss": "npx stylelint \"**/*.scss\"",
        "lint:scss:fix": "npx stylelint \"**/*.scss\" --fix",
        "test:unit": "jest --config ./config/jest/jest.config.ts",
        "test:e2e": "cypress open",
        "test:ui": "npx loki test",
        "test:ui:ok": "npx loki approve",
        "storybook": "start-storybook -p 6006 -c ./config/storybook",
        "storybook:build": "build-storybook -c ./config/storybook",
        "test:ui:report": "npm-run-all --continue-on-error storybook:build test:ui:ci test:ui:json test:ui:html",
        "test:ui:ci": "loki --requireReference --reactUri file:./storybook-static",
        "test:ui:report:ci": "npm run test:ui:json && npm run test:ui:html",
        "test:ui:json": "node scripts/generate-visual-json-report.js",
        "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
        "check": "npm-run-all lint:ts lint:scss test:unit storybook:build test:ui:ci build:prod",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "postinstall": "node ./scripts/clear-cache.js",
        "prepare": "husky install"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "prettier --list-different",
            "eslint"
        ]
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
